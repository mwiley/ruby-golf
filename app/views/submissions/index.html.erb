<p id="notice"><%= notice %></p>

<h1>Listing Submissions</h1>

<table>
  <thead>
    <tr>
      <th>Challenge</th>
      <th>Code</th>
      <th>Time</th>
      <th>Length</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td><%= submission.challenge.title %></td>
        <td><%= submission.code %></td>
        <td><%= submission.time %></td>
        <td><%= submission.length %></td>
        <td><%= link_to 'Show', challenge_submission_path(@challenge, submission) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submission', new_challenge_submission_path %>
